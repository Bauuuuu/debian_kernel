	waveshare32b-tinker.dtbo \
	pps-gpio.dtbo \
	w1-gpio.dtbo \
	pitft35-resistive-tinker.dtbo \
	ds1307.dtbo \
	lirc-rpi.dtbo \
	lirc-toshiba.dtbo \

